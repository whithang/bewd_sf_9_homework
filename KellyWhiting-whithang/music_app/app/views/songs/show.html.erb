<h1> <%= @song.title %> </h1>

<p>Album: <%= @song.album %><br><br>
	Release Date: <%= @song.year %> by <%= @song.artist.name %>
</p>

<%= link_to "Edit Song", edit_artist_song_path(@song.artist, @song), class: 'btn btn-info' %>
<%= link_to "Delete Song", artist_song_path(@song.artist, @song), :confirm => 'Are you sure?', :method => :delete, class: 'btn btn-danger' %>
<br><br><%= link_to "Home", "/", class: 'btn btn-primary' %>