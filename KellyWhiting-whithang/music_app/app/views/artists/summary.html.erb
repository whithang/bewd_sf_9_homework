<table class="table table-striped table-hover">
			<tr class="small">
				<th>Artists <%= link_to 'View All Artists', artists_path, class: "btn btn-default btn-xs" %></th>
				<th>Record Labels  <%= link_to 'View All Record Labels', record_labels_path, class: "btn btn-default btn-xs" %></th>
				<th>Songs  <%= link_to 'View All Songs', songs_path, class: "btn btn-default btn-xs" %></th>
			
			<!-- can't run without some values pluggeed in first.  need to test for null? -->
				<% @artists.each do |artist| %>
				<tr class="small">
						<td><%= link_to 'Details', artist_path(artist), class: "btn btn-default btn-xs" %>
						<%= artist.name %> 
						</td>
						<td>
							<%= link_to 'Details', record_label_path(artist.record_label), class: "btn btn-default btn-xs" %>
						<%= artist.record_label.name %> 
						</td>
						<td> <% artist.songs.each do |song| %>
							<li><%= link_to 'Details', artist_song_path(artist, song), class: "btn btn-default btn-xs" %><%= song.title %> </li>
							<br>
							<% end %>
						</td>
	  				
  				</tr>
				<% end %>
	</table>
	<%= link_to "Add New Artist", new_artist_path, class: 'btn btn-lg btn-danger' %>
	</div>