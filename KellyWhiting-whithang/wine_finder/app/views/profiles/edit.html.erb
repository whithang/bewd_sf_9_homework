<h1>Edit  My Profile %></h1>
<p>Update any fields you want to edit:</p>



<%= form_for @profile do |f| %>

	<ul> Profile Name: <%= f.text_field :first_name %></ul>
	<ul> Profile Name: <%= f.text_field :last_name %></ul>
	<ul> About Me: <%= f.text_field :bio %></ul>
	<ul> Birth Date: <%= f.text_field :birth_date %></ul>
	<ul> Street Address: <%= f.text_field :street %></ul>
	<ul> City: <%= f.text_field :city %></ul>
	<ul> State: <%= f.text_field :state %></ul>
	<ul> Zip: <%= f.text_field :zip %></ul>
	<ul> Profile Photo: <%= f.text_field :photo %></ul>
	<%= f.hidden_field :user_id, value: params[:user_id] %> 
	<ul> <%= f.submit "Update" %></ul><br> 
	<ul> <%= link_to "Home", profile_path(@profile), class: 'btn btn-primary' %></ul>

	<% end %>

	<p><%= javascript_include_tag "application" %> </p>