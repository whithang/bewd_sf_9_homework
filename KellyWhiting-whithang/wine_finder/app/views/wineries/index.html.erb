<h1 style="color:#000000;">Ultimate Wine Country Trip Planner</h1>
<p>
<br><br>
    <%= link_to "Home", "/", class: 'btn btn-default btn-xs' %> 
</p>

<p> 
	<table class="table table-striped table-hover">
			<tr class="small">
				<th><h1>All wineries in Sonoma</h1></th>
			</tr>
			<% @wineries.each do |winery| %>
			<tr class="small">
						<td><ul>
						<%= link_to "#{winery.name}", winery_path(winery) %></ul>
						</td>
			</tr>
			<% end %>
	</table>
</p>